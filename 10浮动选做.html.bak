  <title>Document</title>
  <style>
	*{
	margin:0;
	padding:0;
	}
	.wrap{
	margin-top:30px;
	width:600px;
	height:700px;
	}

	.left-ear{
	float:left;
	width:150px;
	height:150px;
	margin-left:100px;
	border-radius:50%;
	background-color:#000000;
	}
	.right-ear{
	float:right;
	width:150px;
	height:150px;
	background-color:#000000;
	border-radius:50%;
	}
	.face{
	width:450px;
	height:450px;
	margin-left:125px;
	border-radius:50%;
	background-color:#000000;
	padding-top:90px;
	box-sizing:border-box;
	}
	.face .eyes{
	width:300px;
	height:120px;
	margin:0 auto;
	

	}
	.face .eyes .left-eye{
	display:inline-block;
	width:80px;
	height:120px;
	margin-right:60px;
	border-radius:50%;
	background-color:#ffffff;
	}
	.face .eyes .left-eye .leye-cir{
	width:20px;
	height:20px;
	margin-right:115px;
	padding:15px;
	border-radius:50%;
	background-color:#000000;
	}
	.face .eyes .right-eye{
	display:inline-block;
	width:80px;
	height:120px;
	border-radius:50%;
	background-color:#ffffff;
	}
	.face .eyes .right-eye .reye-cir{
	width:20px;
	height:20px;
	padding:15px;
	border-radius:50%;
	background-color:#000000;
	}
	.face .out-mouth{
	width:200px;
	height:50px;
	margin:auto;
	margin-top:75px;
	padding-top:10px;
	border-radius:50%;
	background-color:#ffffff;
	box-sizing:border-box;
	}
	.face .out-mouth .in-mouth{
	width:100px;
	height:30px;
	
	margin:auto;
	border-radius:50%;
	background-color:#ff0000;

	}
  </style>
 </head>
 <body>
	<div class="wrap">
		
		<p class="left-ear"></p>
		<p class="right-ear"></p>
		<div class="face">
			<div class="eyes">
				<div class ="left-eye">
					<p class="leye-cir">
					</p>
				</div>
				<div class="right-eye">
					<p class="reye-cir">
					</p>
				</div>
			</div>
			<div class="out-mouth">
				<p class="in-mouth">
				</p>
			</div>
		</div>
	</div>
